
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Packing List - ASN</h2>
    </div>
    <div class="col-lg-10">
        <div class="ibox-content">
            <form role="form" class="form-inline">
                <div class="form-group">
                    <label>Id Packing List:</label>
                    <input id="txtPackingList" type="text" class="form-control" />
                </div>
                <div class="form-group">
                    <label>PO #:</label>
                    <input id="txtPo" type="text" class="form-control" />
                </div>
                <div class="form-group">
                    <label>Fecha Inicio:</label>
                    <div id='dtFI' class='input-group date'>
                        <span class='input-group-addon'><i class='fa fa-calendar'></i></span>
                        <input type='text' class="form-control">
                    </div>
                </div>
                <div class="form-group">
                    <label>Fecha Fin:</label>
                    <div id='dtFF' class='input-group date'>
                        <span class='input-group-addon'><i class='fa fa-calendar'></i></span>
                        <input type='text' class="form-control">
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row" id="botones">
        <div class="col-lg-7">
            <button class="btn btn-primary" type="button" id="btnSearch">Search</button>
        </div>
    </div>
    <div class="row" id="detalle">
        <div class="col-lg-12">
            <div id="divTexto"></div>
            <div id="divTableContent" class="ibox-content">
            </div>
        </div>
    </div>
</div> <!-- :fin -->

<div class="modal inmodal in" id="mdPL" tabindex="-1" role="dialog" aria-hidden="true" style="display: none; padding-right: 17px;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h5 class="modal-title">Change Factory</h5>
            </div>
            <div class="modal-body">
                <div class="form-inline">
                    
                    <div class="form-group" style="padding-right:20px;">
                        <label style="padding-right:5px;">Packing List #:</label>
                        <input type="text" id="txtIdPackingList" readonly class="form-control" />
                    </div>
                    <div class="form-group" style="padding-right:20px;">
                        <label style="padding-right:5px;">Factory:</label>
                        <select id="cboFabrica" class="form-control" style="width:150px"><option value="">Select</option></select>
                    </div>
                    <div class="form-group" style="padding-right:20px;">
                        <button type="button" class="btn btn-primary" id="btnChangeFactory">Update</button>
                    </div>
                </div>
                <br />
                <div id="divTela" style="max-height:400px;">

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white" id="btnCloseFabricCode" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
