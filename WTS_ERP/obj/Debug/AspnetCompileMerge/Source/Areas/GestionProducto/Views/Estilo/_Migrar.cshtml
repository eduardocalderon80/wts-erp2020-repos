<div class="ibox">
    <div class="ibox-title">
        <div class="ibox-tools">
            <button id="_btnaceptar_migrar" class="btn btn-primary">
                <span class="fa fa-check-circle"></span>
                Accept
            </button>
            <button id="_btncancelar_migrar" class="btn btn-default" data-dismiss="modal">
                Cancel
            </button>
        </div>
    </div>
    <div class="ibox-content">
        <div class="hide">
            <input type="text" id="txtpar_migrarestilo" value="DATA-PARAMETRO" />
        </div>
        <div class="row">
            <div class="form-horizontal">
                <div class="form-group">
                    <div class="col-lg-12 text-center" id="divCabMigrar" >
                                       
                    </div>
                    <div class="col-lg-12 text-center" id="divinfointra" style="color:#1ab394">
                        <label class="control-label"> la información del estilo que se presenta en esta ventana corresponde a intranet </label>
                    </div>
                </div>
                <div class="form-group">                    
                    <label class="control-label col-lg-3">Style:</label>                   
                    <div class="col-lg-9">
                        <input class="form-control" id="txtstyle_migrar" readonly />
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-lg-3">Desc:</label>
                    <div class="col-lg-9">
                       <textarea class="form-control" id="txtdesc_migrar" readonly></textarea>
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-lg-3">Season</label>
                    <div class="col-lg-9">
                        <input class="form-control" id="txtseason_migrar" readonly />
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-lg-3">Version</label>
                    <div class="col-lg-9">
                        <input class="form-control" id="txtversion_migrar" readonly />
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label col-lg-3">Fabric</label>
                    <div class="col-lg-9">
                        <input class="form-control" id="txttela_migrar" readonly />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="ibox-content">
        <div class="form-horizontal">
            <div class="form-group">
                <div class="table-responsive">
                    <table class="table table-bordered table-hover" id="tbl_migrarcolor">
                        <thead>
                            <tr>
                                <th style="width:70%;">Color</th>
                                <th style="width:30%;"></th>
                            </tr>
                        </thead>
                        <tbody id="tbody_migrarcolor"></tbody>
                    </table>
                </div>                
            </div>
        </div>
    </div>
</div>