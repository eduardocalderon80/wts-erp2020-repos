<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="col-sm-6">
                <h2>PDCHART</h2>
            </div>
            <div class="col-sm-6 text-right">
                <button type="button" class="btn btn-primary dim" id="btnexportarexcel">
                    <span class="fa fa-file-excel-o"></span>
                    Export
                </button>

                <button type="button" class="btn btn-success dim" id="btnSearch">Search <i class="fa fa-search"></i> </button>

            </div>
        </div>
    </div>
    <div class="hide">       
        <input type="text" id="txtpar" data-par="DATA-PARAMETRO" value="" />
    </div>
    <div class="panel-body" id="div_body_filtro">
        <div class="form-horizontal">

            <div class="form-group">
                <div class="col-sm-6">
                    <label class="col-sm-2 control-label">Customer</label>
                    <div class="col-sm-8">
                        <select class="form-control _enty" id="cbocliente" data-required="true" data-min="1" data-max="20"></select>
                    </div>
                </div>       
                <div class="col-sm-6">
                    <label class="control-label col-sm-2">Status</label>
                    <div class="col-sm-8">
                        <select class="form-control" id="cboStatus" data-required="true" data-min="1" data-max="20"></select>
                    </div>
                </div>                      
            </div>

            <div class="form-group">
                <div class="col-sm-6">
                    <label class="control-label col-sm-2">Season</label>
                    <div class="col-sm-8">
                        <select class="form-control _enty" id="cboclientetemporada" data-required="true" data-min="1" data-max="20"><option value="">Select Client</option></select>
                    </div>
                </div>

                <div class="col-sm-6">
                    <label class="control-label col-sm-2">Division</label>
                    <div class="col-sm-8">
                        <select class="form-control _enty" id="cboclientedivision" data-required="true" data-min="1" data-max="20"><option value="">Select Client</option></select>
                    </div>
                </div>

            </div>
       
        </div>    
    </div>
    <div class="panel-body">
        <div class="ibox float-e-margins">
            <div class="ibox-content" style="position:relative; width:100%;overflow-x:auto; overflow-y: auto; max-height: 600px; min-height:0px;">
                <table class="table table-bordered table-hover" id="tbl_index">
                    <thead>
                        <tr>
                            <th></th>
                            <th>Style</th>
                            <th>Description</th>
                            <th>Season</th>
                            <th>Division</th>
                            <th>Status</th>                          
                        </tr>
                    </thead>
                    <tbody id="tbody_index"></tbody>
                </table>
            </div>          
        </div>
    </div>
</div>
