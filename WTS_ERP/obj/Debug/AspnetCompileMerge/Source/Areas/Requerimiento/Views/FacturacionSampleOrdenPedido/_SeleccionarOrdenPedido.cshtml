<div id="panelEncabezado_SeleccionarOrdenPedido">
    <div class="hide">
        <input type="text" id="txtpar_SeleccionarOrdenPedido" value="DATA-PARAMETRO" />
        <input type="text" id="hf_idprograma_selectop" class="_enty" data-required="true" data-id="IdPrograma" data-min="1" data-max="20" />
        <input type="text" id="hf_numeroordenpedido_selectop" value="" />
        <input type="text"  id="hf_fechacreacion_op_selectop" value=""/>
        @*<input type="text" id="rutaFileServerEstilo" value="@ViewBag.RutaFileServer" />*@
    </div>
    <div class="form-horizontal row">
        <div class="text-right">
            <button type="button" class="btn btn-default" id="btn_Cancel_SelectOP" data-dismiss="modal">
                <span class="fa fa-ban"></span>
                Cancel
            </button>
            <button type="button" class="btn btn-primary" id="btn_Save_SelectOP">
                <span class="fa fa-save"></span>
                Save
            </button>
            <button type="button" class="btn btn-success" id="btn_SelectOP_SelectOP">
                <span class="fa fa-save"></span>
                Select
            </button>
        </div>
    </div>
    <hr class="modal_hr">
    <div class="form-horizontal" style="max-height:430px; overflow-y: auto;">
        <div class="col-sm-6">
            <div class="form-group">
                <label class="control-label col-sm-4">Supplier</label>
                <div class="col-sm-8">
                    <select class="form-control _enty" id="cbo_proveedor_selectop" data-required="true" data-id="IdProveedor" data-min="1" data-max="20" disabled></select>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-4">Factory Invoice Number</label>
                <div class="col-sm-8">
                    <div class="col-sm-5">
                        <input type="text" class="form-control _enty" id="txt_serie_selectop" data-required="true" data-id="Serie_FacturaFabrica" data-min="1" data-max="20" disabled />
                    </div>
                    <div class="col-sm-1">-</div>
                    <div class="col-sm-6">
                        <input type="text" class="form-control _enty" id="txt_numero_selectop" data-required="true" data-id="Numero_FacturaFabrica" data-min="1" data-max="20" disabled />
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                <label class="control-label col-sm-4">Client</label>
                <div class="col-sm-8">
                    <input type="text" class="form-control _enty" id="txt_cliente_selectop" data-required="true" data-id="NombreCliente" disabled />
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-4">Generation Date</label>
                <div class="col-sm-8">
                    <div class='input-group date cls_grupo_fechageneracion_selectop'>
                        <input id="txtfechageneracion_selectop" type='text' class='form-control _fechageneracion_selectop _enty'
                               data-date-format="mm/dd/yyyy" data-type="date"
                               data-level="true" data-min="10" data-max="10"
                               data-required="true" data-id="FechaGeneracion" disabled>
                        <div class="input-group-addon">
                            <span class="fa fa-calendar"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="form-group _cls_group_validar" id="div_group_txtcomentario">
                <label class="control-label col-sm-2">Comment</label>
                <div class="col-sm-10">
                    <textarea class="form-control _enty" id="txta_comentario_selectop" rows="2" style="resize: none;"
                              data-required="false" data-id="Observacion" data-min="0" data-max="8000"></textarea>
                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group _cls_group_validar" id="div_group_cboordenpedido">
                <label class="control-label col-sm-4">Purchase Order</label>
                <div class="col-sm-8">
                    <select class="form-control _enty" id="cbo_op_selectop" data-idordenpedido-temporal="" data-required="true" data-id="IdOrdenPedido" data-min="1" data-max="20"></select>
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="row">
                <table class="table table-bordered table-hover" id="tbl_selectop_fsample">
                    <thead>
                        <tr>
                            <th>Style Code</th>
                            <th>Sample</th>
                            <th>V</th>
                            <th>Color Combo</th>
                            <th>Size</th>
                            <th>Counter Sample will be Req</th>
                            <th>Sample to be Billed</th>
                            <th>Sample Price</th>
                            <th>Counter Sample to be Billed</th>
                            <th>Couter Sample Price</th>
                        </tr>
                    </thead>
                    <tbody id="tbody_selectop_fsample"></tbody>
                </table>
            </div>
        </div>
        
        <div class="col-sm-12">
            <div class="form-group">
                <label class="col-sm-1 col-sm-offset-9">Total</label>
                <div class="col-sm-2">
                    <input type="text" class="form-control" id="txt_total_selectop" disabled />
                </div>
            </div>
        </div>
    </div>

</div>