                                    <label class="col-sm-3 control-label">Color:</label>                                   
                                    <div class="col-sm-9">
                                        <input id="txtColor" type="text" class="form-control" readonly />
                                    </div>
                                </div>
                            </div>
                            <div class="form-horizontal row">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Status:</label>
                                    <div class="col-sm-9">
                                        <select id="cboStatusLabDip" class="form-control"></select>
                                    </div>
                                </div>
                            </div>
                            <div class="form-horizontal row">
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Date:</label>
                                    <div class="col-sm-9">
                                        <div id='dtLabDipStatus' class='input-group date'>
                                            <span class='input-group-addon'><i class='fa fa-calendar'></i></span>
                                            <input type='text' class='form-control'>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-horizontal row">
                                <div class="form-group">
                                    <div class="col-sm-12 control-label text-right">
                                        <button type="button" class="btn btn-outline btn-primary" id="btnSave" onclick="AgregarLabdipStatus();">
                                            <span class="fa fa-plus"></span>
                                       </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-sm-6">
                    <div class="ibox">
                        <div class="ibox-content" style="border: none">
                            <div class="form-horizontal">
                                <div id="_iddivApproval" class="_divApproval hide">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label input-sm m-b-xs">Alternative</label>
                                        <div class="col-sm-9">
                                            <input id="txtAlternativa" type="text" onkeyup="Mayus(this)" class="form-control _enty input-sm m-b-xs" data-required="true">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label input-sm m-b-xs">Dyeing House Code</label>
                                        <div class="col-sm-9">
                                            <input id="txtDyeingCode" type="text" class="form-control _enty input-sm m-b-xs" onkeyup="Mayus(this)" data-required="true">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label input-sm m-b-xs">Comment</label>
                                        <div class="col-sm-9">
                                            <input id="txtComment" type="text" class="form-control _enty input-sm m-b-xs" onkeyup="Mayus(this)" data-required="true">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label input-sm m-b-xs">Solidez a la luz</label>
                                        <div class="col-sm-9">
                                            <input id="txtsolidezluz" type="text" class="form-control _enty input-sm m-b-xs" onkeypress="return DigitimosDecimales(event, this)" data-type="decimal" maxlength="3" data-required="true">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label input-sm m-b-xs">Solidez al Frote Humedo</label>
                                        <div class="col-sm-9">
                                            <input id="txtsolidezfrotehumedo" type="text" class="form-control _enty input-sm m-b-xs" onkeypress="return DigitimosDecimales(event, this)" data-type="decimal" maxlength="3" data-required="true">
                                        </div>
                                    </div>
                                    <div class="form-group">                                        
                                            <label class="col-sm-3 control-label input-sm m-b-xs">Solidez al Frote Seco</label>                                       
                                        <div class="col-sm-9">
                                            <input id="txtsolidezfroteseco" type="text" class="form-control _enty input-sm m-b-xs" onkeypress="return DigitimosDecimales(event, this)" data-type="decimal" maxlength="3" data-required="true">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <br />
                <table id="tblLabdipStatus" class="table table-hover table-bordered">
                    <thead>
                        <tr>
                            <th>Status</th>
                            <th>Date</th>
                            <th>Alternativa</th>
                            <th>Dyeing House Code</th>
                            <th>Comment</th>
                            <th>S. Luz</th>
                            <th>S.F. Humedo</th>
                            <th>S.F. Seco</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white" id="btnCloseFabricCode" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

