<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row" style="vertical-align: middle;">
            <div class="col-sm-6" id="divtituloprincipal">
                <h2>Fabric</h2>
            </div>
            <div class="col-sm-6 text-right">
                <button type="button" class="btn btn-primary dim" id="btnSave">Save</button>
                <button type="button" class="btn btn-primary dim" id="btnnFabricCode">Fabric Code</button>
                <button type="button" class="btn btn-danger dim" id="btnCancel">Cancel</button>
            </div>
        </div>
    </div>

    <div class="panel-body">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                    <h4>Header</h4>
                            </div>
            <div class="ibox-content">
                <div class="form-horizontal">
                    <div class="form-group">
                        <label class="control-label col-sm-2" for="cboCliente">* Client:</label>
                            <div class="col-sm-8">
                                <input id="txtCliente" class="form-control" readonly />
                                <select id="cboCliente" class="form-control" data-initial=""><option value="">Select one</option></select>
                            </div>
                        </div>
                        <div class="form-group">
                        <label class="control-label col-sm-2" for="cboSeason">* Season:</label>
                            <div class="col-sm-8">
                                <select id="cboSeason" class="form-control" data-initial=""><option value="0">Select one</option></select>
                            </div>
                        </div>
                    <div id="divCodigo" class="form-group">
                        <label class="control-label col-sm-2" for="txtCode">Code:</label>
                        <div class="col-sm-8">
                            <input id="txtCode" class="form-control" readonly />
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="ibox float-e-margins">
            <div class="ibox-title">
                    <h4>Detail</h4>
            </div>
            <div class="ibox-content">
                <div id="divFabric1" class="form-horizontal divFabric" data-divProcess="divFabricProcess1" data-type="">
                    <div class="form-group">
                        <label class="lblSupplierReq control-label col-sm-2">Supplier:</label>
                        <div class="col-sm-8">
                            <div id="div_cboProveedor">
                                <select id="cboFabricSupplier" class="cboFabricSupplier form-control" data-initial=""><option value="">Select One</option></select>
                            </div>
                            
                            <input type="text" class="form-control" id="txtProveedor" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="lblConstructionReq control-label col-sm-2">Construction:</label>
                        <div class="col-sm-8">
                            <textarea id="txtConstruction" onkeyup="Mayus(this)"  class="form-control FabricConstruction" rows="2" cols="8" data-initial=""></textarea>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="lblYarnReq control-label col-sm-2">Yarn:</label>
                        <div class="col-sm-8">
                            <input id="txtYarn" onkeyup="Mayus(this)"  class="form-control FabricYarn" data-initial="" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="lblContentReq control-label col-sm-2">Content:</label>
                        <div class="col-sm-8">
                            <input id="txtContent" onkeyup="Mayus(this)"  class="form-control FabricContent" data-initial="" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="lblWeightReq control-label col-sm-2">Weight:</label>
                        <div class="col-sm-5">
                            <input id="txtWeight" data-initial="" class="form-control Fabricweight" value="0" type="text" onblur="ValidarNumero(this);" onkeypress="return DigitimosDecimales(event, this)" />
                        </div>
                        <div class="col-sm-3">
                            <select id="cboLavado" data-initial="" class="form-control FabricLavado"><option>AW</option><option>BW</option></select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">Status:</label>
                        <div class="col-sm-8">
                            <select id="cboStatus" data-initial="" class="form-control FabricStatus">
                                <option value="P">Pending</option>
                                <option value="A">Approved</option>
                                <option value="R">Rejected</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">Comments:</label>
                        <div class="col-sm-8">
                            <input id="txtComents" onkeyup="Mayus(this)" data-initial="" class="form-control FabricComent" />
                        </div>
                    </div>
                </div>
                <div class="col-sm-12" id="div_principal_proceso">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h4>Process</h4>
                        </div>
                        <div class="panel-body">
                            <div class="form-horizontal">
                               
                                <div class="row">
                                   <div class="col-sm-6">
                                       <div class="form-group">
                                           <label class="control-label col-sm-2">Dyeing Process</label>
                                           <div class="col-sm-10">
                                               <select class="form-control" id="cboDyeProcess"></select>
                                           </div>
                                       </div>
                                   </div>
                               </div>
                               
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="form-group" id="div_grupocomboproceso">
                                            <label class="control-label col-sm-2">Aditional Fabric Process</label>
                                            <div class="col-sm-10">
                                                <select class="form-control" id="cbo_proceso_add"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-group">
                                            <div class="col-sm-1">
                                                <button type="button" class="btn btn-sm btn-primary" id="btn_addproceso">
                                                    <span class="fa fa-plus-square"></span>
                                                    Add
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                             
                            </div>
                            <div class="row">
                            <div class="col-sm-12">
                                    <table class="table table-bordered" id="tblprocesos" data-estadomodificado="0">
                                        <thead>
                                            <tr>
                                                <th class="col-sm-1"></th>
                                                <th class="col-sm-5 text-center">Process</th>
                                                <th class="col-sm-6 text-center">Comment</th>
                                            </tr>
                                        </thead>
                                        <tbody id="tbody_process"></tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <div class="row">
                    <div class="col-sm-6">
                        <h3 class="col-sm-3 font-bold">Color</h3>
                        <div class="col-sm-3 hide">
                            <button type="button" class="btn btn-sm btn-warning" id="btnReporteResumenLabdip">
                                <span class="fa fa-print"></span>
                                Print
                            </button>
                        </div>
                    </div>
                    <div class="col-sm-6 text-right">                        
                        <button type="button" class="btn btn-sm btn-info" id="btn_addlabdip">
                            <span class="fa fa-plus-square"></span>
                            Add
                        </button>
                        <button type="button" class="btn btn-primary btn-sm" id="btnsavelabdip">
                            <span class="fa fa-save"></span>
                            Save
                        </button>
                    </div>
                </div>
            </div>
            <div class="ibox-content">
                <div style="max-height: 400px; overflow-y:auto;">
                    <table id="tblLabDip" class="table table-hover table-bordered">
                        <thead>
                            <tr class="text-center">
                                <td style="width:2%"></td>
                                <td class="hide"></td>
                                <th style="width:20%">Color (*)</th>
                                <th style="width:14%">Standard</th>
                                <th style="width:10%">Type (*)</th>
                                <th style="width:10%">Approval By (*)</th>
                                <th  style="width:30%">Dyeing House (*)</th>
                                <th class="hide">Process</th>
                                <th style="width:10%">Season (*)</th>
                                <th style="width:2%">Status</th>
                                <th style="width:2%"></th>
                            </tr>
                        </thead>
                        <tbody id="tbody_labdip"></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


<div class="modal inmodal in" id="mdStatus" tabindex="-1" role="dialog" aria-hidden="true" style="display: none; padding-right: 17px;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content animated bounceInRight">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h5 class="modal-title">LabDip Status</h5>
            </div>
