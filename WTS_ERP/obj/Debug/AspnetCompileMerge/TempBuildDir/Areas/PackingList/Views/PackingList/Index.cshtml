<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="col-sm-6">
                <h2>Packing List</h2>
            </div>
            <div class="col-sm-6 text-right">
                <button type="button" class="btn btn-primary dim" id="btnNewPL">
                    <span class="fa fa-file-o"></span>&nbsp; New PL                    
                </button>
            </div>
        </div>

    </div>
    <div class="panel-body">
        <div class="form-horizontal">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="form-horizontal">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="control-label col-sm-2">
                                    Factory
                                </label>
                                <div class="col-sm-10">
                                    <select class="form-control" id="cboFabrica">                                        
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-2">Customer</label>
                                <div class="col-sm-10">
                                    <select class="form-control" id="cboCliente">                                    
                                    </select>
                                </div>
                            </div>
                            <div class="form-group" id="dtFI">
                                <label class="control-label col-sm-2">Start Date</label>
                                <div  class='input-group date col-sm-10'>
                                    <span class='input-group-addon'><i class='fa fa-calendar'></i></span>
                                    <input type='text'  id="txtFI" class='form-control'>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="control-label col-sm-2"># Packing  List</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" value="" id="txtCodigoPackingList" />
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="control-label col-sm-2"># Po</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" value="" id="txtPo" />
                                </div>
                            </div>

                            <div class="form-group" id='dtFF'>
                                <label class="control-label col-sm-2">End Date</label>
                                <div  class='input-group date col-sm-10' >
                                    <span class='input-group-addon'><i class='fa fa-calendar'></i></span>
                                    <input type='text' id="txtFF" class='form-control'>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-footer text-right">
                    <button type="button" class="btn btn-success" id="btnSearch">
                        <span class="fa fa-search"></span>
                        Search
                    </button>
                </div>
            </div>
        </div>
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Details</h5>
            </div>
            <div id="divContent" class="ibox-content">                
            </div>
        </div>
    </div>
</div>
