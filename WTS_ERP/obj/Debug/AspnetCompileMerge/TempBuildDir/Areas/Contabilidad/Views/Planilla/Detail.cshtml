<div class="row wrapper white-bg page-heading">
    <div id="content_header" class="ibox float-e-margins">
        <div id="divtituloprincipal" class="col-sm-6">
            <h2 class="font-bold">Planilla</h2>
        </div>
    </div>
</div>

<div class="hide">
    <input type="text" id="txtpar" value="DATA-PARAMETRO" />
</div>

<div class="row wrapper-content gray-bg animated">

    <div id="content_form" class="ibox">
        <div class="ibox-title">
            <div class="form-horizontal row ">
                <div class="col-sm-6">
                    <h3 id="_title" class="text-navy bold">Details</h3>
                </div>
                <div class="col-sm-6 text-right">
                    <button type="button" class="btn btn-default" id="btnReturn">
                        <span class="fa fa-reply-all"></span>
                        Return
                    </button>
                </div>
            </div>
        </div>
        <div class="ibox-content">
            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Document:</label>
                        <div class="col-sm-9">                            
                            <input class="form-control _enty" disabled name="IdDocumento" id="txtDocumento" data-min="" data-max="4" data-required="true" data-type="int" data-id="IdAnio" maxlength="4">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Total Employee:</label>
                        <div class="col-sm-9">
                            <input class="form-control _enty" disabled name="IdDocumento" id="txtEmpleado" data-min="" data-max="4" data-required="true" data-type="int" data-id="IdAnio" maxlength="4">
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Month:</label>
                        <div class="col-sm-9">                            
                            <input class="form-control _enty" disabled name="IdAnio" id="txtMes" data-min="" data-max="4" data-required="true" data-type="int" data-id="IdAnio" maxlength="4">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Year:</label>
                        <div class="col-sm-9">                            
                            <input class="form-control _enty" disabled name="IdAnio" id="txtAnio" data-min="" data-max="4" data-required="true" data-type="int" data-id="IdAnio" maxlength="4">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Total Descuento:</label>
                        <div class="col-sm-9">
                            <input class="form-control _enty" disabled name="IdAnio" id="txtTotalDescuento" data-min="" data-max="4" data-required="true" data-type="int" data-id="IdAnio" maxlength="4">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Total Net:</label>
                        <div class="col-sm-9">
                            <input class="form-control _enty" disabled name="IdAnio" id="txtTotalNeto" data-min="" data-max="4" data-required="true" data-type="int" data-id="IdAnio" maxlength="4">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="content_list" class="ibox">
        <div class="ibox-title hide">
            <h5>Details</h5>
        </div>
        <div id="tabledetails" class="ibox-content">
            <div class="table-responsive">
                <input type="text" class="form-control input-sm m-b-xs" id="filter"
                       placeholder="Search in table">
                <table style="width:100%" height="100" class="footable table table-bordered"  data-page-size="30" data-filter=#filter id="contenTableDetails">
                    <thead>
                        <tr>
                            <th class='col-sm-2' data-sort-ignore="true"></th>
                            <th class='col-sm-2' data-sort-ignore="true">Trabajador</th>
                            <th class='col-sm-2' data-sort-ignore="true">DNI</th>
                            <th class='col-sm-2' data-sort-ignore="true">Total Discount</th>
                            <th class='col-sm-2' data-sort-ignore="true">Total Net</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                    <tfoot>
                        <tr>
                            <td colspan="8">
                                <ul class="pagination pull-right"></ul>
                            </td>
                        </tr>
                    </tfoot>
                </table>

            </div>
        </div>
    </div>
</div>
