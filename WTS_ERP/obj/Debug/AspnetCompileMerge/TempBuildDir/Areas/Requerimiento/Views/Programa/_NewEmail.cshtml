<div id="panelEncabezado_NewEmail">
    <div class="hide">
        <input type="text" id="newemail_txtpar" value="DATA-PARAMETRO" />
        <input type="text" id="rutaFileServerTelaEmail" value="@ViewBag.RutaFileServerTela" />
        <input type="text" id="rutaFileServerAvioEmail" value="@ViewBag.RutaFileServerAvio" />
        <input type="text" id="rutaFileServerColorEmail" value="@ViewBag.RutaFileServerColor" />
        <input type="text" id="rutaFileServerEstiloEmail" value="@ViewBag.RutaFileServerEstilo" />
        <input type="text" id="rutaFileServerArteEmail" value="@ViewBag.RutaFileServerArte" />
    </div>
    <div class="form-horizontal row">
        <div class="col-sm-12 m-b">
            <div class="text-right">
                <button type="button" class="btn btn-default" id="btnModalClose" data-dismiss="modal">
                    <span class="fa fa-ban"></span>
                    Close
                </button>
                <button type="button" class="btn btn-primary" id="btnSendEmail">
                    <span class="fa fa-send"></span>
                    Send
                </button>
            </div>
            <hr class="modal_hr">
        </div>
    </div>
    <div class="form-horizontal row">
        <div class="col-sm-3">
            <div class="has-float-label m-b" id="div_details_combo">
                <select class="form-control" id="cboSelectReq" multiple="multiple"></select>
                <label class="select2-label" id="lblSelectReq" for="cboSelectReq"></label>
            </div>
            <div class="feed-activity-list" id="div_details_info"></div>
        </div>
        <div class="col-sm-9">
            <div class="mail-box email-scroll">
                <div class="mail-body" style="padding: 15px;">
                    <form class="form-horizontal" method="get">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">To:</label>
                            <div class="col-sm-10">
                                <input class="tagsinput form-control" id="cboCorreosEmail" type="text" value="jvaldez@wts.com.pe" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">CC:</label>
                            <div class="col-sm-10">
                                <input class="tagsinput form-control" id="cboCCEmail" type="text" value="jvaldez@wts.com.pe" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Subject:</label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtSubjectEmail" value="">
                            </div>
                        </div>
                    </form>
                </div>
                <div class="mail-text h-200">
                    <div class="summernote" id="summernote_email"></div>
                </div>
                <div class="mail-body text-right tooltip-demo" style="padding: 10px;">
                    <button type="button" class="btn btn-danger btn-sm" id="btnDiscardEmail">
                        <span class="fa fa-times"></span>
                        Discard
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>