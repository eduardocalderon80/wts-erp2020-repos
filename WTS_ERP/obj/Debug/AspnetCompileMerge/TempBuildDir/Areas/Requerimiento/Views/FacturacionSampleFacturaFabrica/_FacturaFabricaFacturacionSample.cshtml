<div id="panelEncabezado_FacturaFabricaFacturacionSample">
    <div class="hide">
        <input type="text" id="txtpar_FacturaFabricaFacturacionSample" value="DATA-PARAMETRO" />
        <input type="text" id="hf_idprograma_ff" class="_enty" data-required="true" data-id="IdPrograma" data-min="1" data-max="20" />
        <input type="text" id="hf_idfacturafabrica" class="_enty"  value="0" data-required="false" data-id="IdFacturaFabrica" data-min="1" data-max="20" />
        @*<input type="text" id="rutaFileServerEstilo" value="@ViewBag.RutaFileServer" />*@
    </div>
    <div class="form-horizontal row">
        <div class="text-right">
            @*data-dismiss="modal"*@
            <button type="button" class="btn btn-default" id="btnFacturaFabrica_Cancel">
                <span class="fa fa-ban"></span>
                Cancel
            </button>
            <button type="button" class="btn btn-primary" id="btn_SaveFacturaFabrica">
                <span class="fa fa-save"></span>
                Save
            </button>
        </div>
    </div>
    <hr class="modal_hr">
    <div class="form-horizontal row" style="max-height:430px; overflow-y: auto;">
        <div class="col-sm-6">
            <div class="form-group">
                <label class="control-label col-sm-2">Supplier</label>
                <div class="col-sm-10">
                    <select class="form-control _enty _cls_disabled_edit" id="cbo_proveedor_ff" data-required="true" data-id="IdProveedor" data-min="1" data-max="20"></select>
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Serie-Numero</label>
                <div class="col-sm-10">
                    <div class="col-sm-5">
                        <input type="text" class="form-control _enty _cls_disabled_edit" id="txt_serie_ff" data-required="true" data-id="Serie_FacturaFabrica" data-min="1" data-max="20" />
                    </div>
                    <div class="col-sm-1">-</div>
                    <div class="col-sm-6">
                        <input type="text" class="form-control _enty _cls_disabled_edit" id="txt_numero_ff" data-required="true" data-id="Numero_FacturaFabrica" data-min="1" data-max="20" />
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group">
                <label class="control-label col-sm-2">Client</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control _enty" id="txt_cliente_ff" data-required="true" data-id="NombreCliente" disabled />
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-sm-2">Generation Date</label>
                <div class="col-sm-10">
                    <div class='input-group date cls_grupo_fechageneracion_ff'>
                        <input id="txtfechageneracion_ff" type='text' class='form-control _fechageneracion_ff _enty _cls_disabled_edit' 
                               data-date-format="mm/dd/yyyy" data-type="date" 
                               data-level="true" data-min="10" data-max="10"
                               data-required="true" data-id="FechaGeneracion">
                        <div class="input-group-addon">
                            <span class="fa fa-calendar"></span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="form-group">
                <label class="control-label col-sm-1">Comment</label>
                <div class="col-sm-11">
                    <textarea class="form-control _enty" id="txta_comentario_ff" rows="2" style="resize: none;"
                              data-required="false" data-id="Observacion" data-min="0" data-max="8000"></textarea>
                </div>
            </div>
        </div>
        <div class="col-sm-6">
            <div class="form-group" id="div_grupo_cbo_ordenpedido">
                <label class="control-label col-sm-2">Purchase Order</label>
                <div class="col-sm-6">
                    <select class="form-control _enty _cls_disabled_edit" id="cbo_op_ff" data-idordenpedido-temporal="" data-required="true" data-id="IdOrdenPedido" data-min="1" data-max="20"></select>
                </div>
            </div>
        </div>

        <div class="col-sm-12">
            <div class="row">
                <table class="table table-bordered table-hover" id="tbl_facturafabrica_fsample">
                    <thead>
                        <tr>
                            <th>Style Code</th>
                            <th>Sample</th>
                            <th>V</th>
                            <th>Color Combo</th>
                            <th>Size</th>
                            <th>Counter Sample will be Req</th>
                            <th>Sample to be Billed</th>
                            <th>Sample Price</th>
                            <th>Counter Sample to be Billed</th>
                            <th>Couter Sample Price</th>
                        </tr>
                    </thead>
                    <tbody id="tbody_facturafabrica_fsample"></tbody>
                </table>
            </div>
        </div>
        
        <div class="col-sm-12">
            <div class="form-group">
                <label class="col-sm-1 col-sm-offset-9">Total</label>
                <div class="col-sm-2">
                    <input type="text" class="form-control" id="txt_total_ff" disabled />
                </div>
            </div>
        </div>
    </div>
    
</div>