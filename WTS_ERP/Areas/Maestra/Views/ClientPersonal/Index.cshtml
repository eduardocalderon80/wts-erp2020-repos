<div class="row wrapper white-bg page-heading">
    <div id="content_header" class="ibox float-e-margins">
        <div id="divtituloprincipal" class="col-sm-6">
            <h2 class="font-bold">Client by Personal</h2>
        </div>
    </div>
</div>

<div class="hide">
    <input type="text" id="txtpar" data-par="DATA-PARAMETRO" value="" />
</div>

<div class="row wrapper-content gray-bg animated">    
    <div id="divformulario" class="ibox">

        <div class="ibox-title">
            <div class="form-horizontal row ">
                <div id="divTitulo" class="col-sm-6">
                    <h3 id="_title" class="text-navy bold">New</h3>
                </div>
                <div id="divBotonesprimarios" class="col-sm-6 text-right">
                    <button id="btnsave" type="button" class="btn btn-primary">
                        <span class="fa fa-save"></span>
                        Save
                    </button>
                </div>
            </div>
        </div>

        <div id="formMante" class="ibox-content">

            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-2">Personal:</label>
                        <div class="col-sm-10">
                            <select class="select2_Personal form-control _enty" name="IdPersonalxGrupoPersonal" id="cboPersonal" style="width:100%" data-min="1" data-required="true" data-max="100" data-id="IdPersonalxGrupoPersonal"></select>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-12">
                    <div class="ibox">
                        <div class="ibox-title">
                            <h4>Client</h4>
                        </div>
                        <div class="ibox-content">
                            <div class="form-horizontal">
                                <input type="text" class="form-control input-sm m-b-xs" id="filterClient" placeholder="Search in table">
                            </div>
                            <div id="tablaAprobador" class="form-horizontal">
                                <div class="table-responsive">
                                    <table class="table table-bordered" data-page-size="10" data-filter=#filterClient id="contentTableClient">
                                        <thead>
                                            <tr>                                                
                                                <th class='cols-sm-2 text-center' data-sort-ignore="true">Selection</th>
                                                <th class='cols-sm-4 text-center' data-sort-ignore="true">Name Client</th>
                                                <th class='cols-sm-3 text-center' data-sort-ignore="true">Development</th>
                                                <th class='cols-sm-3 text-center' data-sort-ignore="true">Production</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                        <tfoot>
                                            @*<tr>
                                                <td colspan="8">
                                                    <ul class="pagination pull-right"></ul>
                                                </td>
                                            </tr>*@
                                        </tfoot>
                                    </table>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
               
            </div>

        </div>

    </div>
</div>

