<div class="ibox" id="panelEncabezado_newhilado">
    <div class="ibox-title">
        <div class="ibox-tools">
            <button id="_btnSave_newhilado" class="btn btn-primary">
                Save
            </button>
            <button id="_btnCancelar_newhilado" class="btn btn-default" data-dismiss="modal">
                Cancel
            </button>
        </div>
    </div>
    <div class="" style="overflow-y: auto; max-height: 850px;">
        <div class="hide">
            <input type="text" id="txtpar_newhilado" value="DATA-PARAMETRO" />
        </div>
        <div class="ibox">
            <div class="ibox-content">
                <div class="panel-body">
                    <div class="form-horizontal">
                        <!--FILA 1-->
                        <div class="col-sm-12">
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label col-sm-2">System</label>
                                    <div class="col-sm-10">
                                        <select class="form-control _enty _enty_titulo" id="_cbo_sistema_newhilado" data-required="true" data-min="1" data-max="20" data-id="IdSistemaTitulacion"></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--FILA 2-->
                        <div class="col-sm-12">
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Title</label>
                                    <div class="col-sm-10">
                                        <div class="input-group">
                                            @*<select class="form-control _enty" id="_cbo_titulo_newhilado" data-required="true" data-min="1" data-max="20" data-id="IdTituloHiladoTela"></select>*@
                                            <input type="text" class="form-control _enty" id="_txtinput_cbo_titulo_newhilado" name="name_cbo_titulo_newhilado" data-required="true" data-min="1" data-max="2000" data-id="IdTituloHiladoTela" list="_cbo_titulo_newhilado" />
                                            <datalist id="_cbo_titulo_newhilado"></datalist>
                                            <span class="btn btn-sm btn-success input-group-addon" id="btn_new_titulo_newhilado">
                                                <span class="fa fa-plus-circle"></span>
                                            </span>
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group hide" id="div_grupo_titulo_newhilado">
                                    <div class="input-group">
                                        <input type="text" class="form-control _enty_titulo" id="txtnombretitulo" data-required="true" data-min="1" data-max="50" data-id="nombretitulohiladotela" />
                                        <span class="btn btn-sm btn-success input-group-addon" id="btnsave_new_titulo">
                                            <span class="fa fa-save"></span>
                                            Save
                                        </span>
                                        <span class="btn btn-sm btn-danger input-group-addon" id="btncerrar_newtitulo">
                                            <span class="fa fa-close"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--FILA 3-->
                        <div class="col-sm-12">
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <label class="control-label col-sm-2">Shapped yarn</label>
                                    <div class="col-sm-10">
                                        <div class="input-group">
                                            @*<select class="form-control _enty" id="_cbo_formahilado_newhilado" data-required="false" data-min="1" data-max="20" data-id="IdFormaHilado"></select>*@
                                            <input type="text" class="form-control _enty" id="_txtinput_cbo_formahilado_newhilado" name="name_cbo_formahilado_newhilado" data-required="false" data-min="1" data-max="2000" data-id="IdFormaHilado" list="_cbo_formahilado_newhilado" />
                                            <datalist id="_cbo_formahilado_newhilado"></datalist>
                                            <span class="btn btn-sm btn-success input-group-addon" id="btn_new_formahilado_newhilado">
                                                <span class="fa fa-plus-circle"></span>
                                            </span>
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group hide" id="div_grupo_formahilado_newhilado">
                                    <div class="input-group">
                                        <input type="text" class="form-control _enty_formahilado" id="txtnombreformahilado" data-required="true" data-min="1" data-max="100" data-id="nombreformahilado" />
                                        <span class="btn btn-sm btn-success input-group-addon" id="btnsave_new_formahilado">
                                            <span class="fa fa-save"></span>
                                            Save
                                        </span>
                                        <span class="btn btn-sm btn-danger input-group-addon" id="btncerrar_newformahilado">
                                            <span class="fa fa-close"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                </div>
            </div>
        </div>
        <div class="ibox">
            <div class="ibox-title">
                <div class="row">
                    <div class="col-sm-6">
                        <h4>Raw material</h4>
                    </div>
                    <div class="col-sm-6 text-right">
                        <button class="btn btn-sm btn-primary" id="_btn_add_item_materiaprima_newhilado">Add</button>
                    </div>
                </div>
            </div>
            <div class="ibox-content">
                <div class="row">
                    <div id="div_tbl_newhilado" style="max-height:300px; overflow-y:auto;">
                        <table class="table table-bordered table-hover" id="_tbl_newhilado">
                            <thead>
                                <tr>
                                    <th></th>
                                    <th class="text-center">Raw Material</th>
                                    <th class="text-center">%</th>
                                    <th class="text-center">Yarn type</th>
                                    <th class="text-center">Color</th>
                                </tr>
                            </thead>
                            <tbody id="tbody_newhilado"></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>