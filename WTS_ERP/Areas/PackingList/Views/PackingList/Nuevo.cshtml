<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="col-sm-6">
                <h2>Packing List - New</h2>
            </div>
            <div class="col-sm-6 text-right">
                <button type="button" class="btn btn-primary dim" id="btnSave">
                    <span class="fa fa-save"></span>
                    Save
                </button>
            </div>
        </div>
    </div>
    <div class="panel-body">
        <div class="form-horizontal">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="form-horizontal">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="control-label col-sm-2">
                                    Customer
                                </label>
                                <div class="col-sm-10">
                                    <select class="form-control" id="cboCliente"></select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label col-sm-2"># Po</label>
                                <div class="col-sm-10">
                                    <input type="text" class="form-control" value="" id="txtPo" />
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="control-label col-sm-2">Factory</label>
                                <div class="col-sm-10">
                                    <select class="form-control" id="cboFabrica"></select>
                                </div>
                            </div>
                            <div class="form-group">

                            </div>
                        </div>
                    </div>
                </div>
                <div class="panel-footer text-right">
                    <button type="button" class="btn btn-success" id="btnSearch">
                        <span class="fa fa-search"></span>
                        Search
                    </button>
                    
                </div>
            </div>
        </div>
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Details</h5>
            </div>
            <div id="divContent" class="ibox-content">
                <div id="divDestinoH">
                    <h4>Destination:</h4>
                    <h5>Other? &nbsp;<input type="checkbox" class="js-switch" onchange="LoadOtherDestination(this);" /></h5>
                </div>
                <div id="divDestinos"></div>
                <div id="divDestino">
                    <div class="form-horizontal">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label class="control-label col-sm-2">Ship To :</label>
                                <div class="col-sm-10">
                                    <select class="form-control" id="cboClienteDireccion"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <br />
            <div id="divDetalle"></div>
        </div>
    </div>
</div>
<input type="hidden" id="hfIdPackingList" value="0" />
<input type="hidden" id="hfIteracion" value="0" />
<input type="hidden" id="hfOtherDestination" value="0" />