<div class="row wrapper white-bg page-heading">
    <div id="content_header" class="ibox float-e-margins">
        <div id="divtituloprincipal" class="col-sm-6">
            <h2 class="font-bold">Courier Guide</h2>
        </div>
    </div>
</div>

<div class="row wrapper-content gray-bg animated">

    <div id="divformulario" class="ibox">

        <div class="ibox-title">
            <div class="form-horizontal row ">
                <div id="divTitulo" class="col-sm-6">
                    <h3 id="_title" class="text-navy bold">Nuevo</h3>
                </div>
                <div id="divBotonesprimarios" class="col-sm-6 text-right">
                    <button id="btnsave" type="button" class="btn btn-primary">
                        <span class="fa fa-save"></span>
                        Save
                    </button>
                    <button id="btnreturn" type="button" class="btn btn-default">
                        <span class="fa fa-reply-all"></span>
                        Return
                    </button>
                </div>
            </div>
        </div>

        <div class="ibox-content">

            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3"># Guide:</label>
                        <div class="col-sm-9">
                            <input type="text" id="txtnumeroguia" name="status" value="" placeholder="Number Guide" class="form-control" onkeyup="event_search(event)">
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Courier:</label>
                        <div class="col-sm-9">
                            <select id="cbocourier" class="form-control _enty select2_courier" style="width:100%" name="idcourier" data-id="idcourier" data-required="true"></select>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Type Send:</label>
                        <div class="col-sm-9">
                            <select id="cbotipoenvio" class="form-control _enty select2_tipoenvio" style="width:100%" name="idtipoenvio" data-id="idtipoenvio" data-required="true"></select>
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Type:</label>
                        <div class="col-sm-9">
                            <select id="cbotipo" class="form-control _enty select2_tipo" style="width:100%" name="idtipo" data-id="idtipo" data-required="true"></select>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Name:</label>
                        <div class="col-sm-9">
                            <select id="cbonombre" class="form-control _enty select2_nombre" style="width:100%" name="idtipodata" data-id="idtipodata" data-required="true"></select>
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Assume:</label>
                        <div class="col-sm-9">
                            <select id="cboasume" class="form-control _enty select2_asume" style="width:100%" name="idasume" data-id="idasume" data-required="true"></select>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Responsible:</label>
                        <div class="col-sm-9">
                            <select id="cboresponsable" class="form-control _enty select2_responsable" style="width:100%" name="idpersonal" data-id="idreponsable" data-required="true"></select>
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Weight:</label>
                        <div class="col-sm-9">
                            <input id="txtAccion" class="form-control _enty" type="text" name="Accion" data-id="Accion" data-required="true" data-min="1" data-max="100">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Description:</label>
                        <div class="col-sm-9">
                            <input id="txtClase" class="form-control _enty" type="text" rows="6" style="resize:none" name="Clase" data-id="Clase" data-required="true" data-min="1" data-max="200">                            
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-horizontal row hide">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Assume:</label>
                        <div class="col-sm-9">
                            <select id="cboasumewts" class="form-control _enty select2_asume" style="width:100%" name="idasume" data-id="idasume" data-required="true"></select>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div id="inforesponsable" class="form-group">
                    </div>
                </div>
            </div>
            
            <div class="form-horizontal row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <div class="col-sm-3 text-right ">
                            <span class="btn btn-warning btn-file text-right ">
                                <span class="fa fa-upload"></span> Excel File
                                <input class="text-right" type="file" id="inputFile" accept=".xlsx" onchange="fn_validfile(this);" />
                            </span>
                        </div>
                        <div class="col-sm-9">
                            <input class="form-control" disabled="disabled" id="fileSelected" />
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <div id="divBotonesSecundarios" class="col-sm-12 text-right">                         
                            <button id="btneliminar" type="button" title="Eliminar" class="btn btn-outline btn-danger">
                                <span class="fa fa-trash"></span>
                            </button>
                            <button id="btnlimpiar" type="button" title="Limpiar" class="btn btn-outline btn-default">
                                <span class="fa fa-file"></span>
                            </button>
                        </div>
                    </div>
                </div>
            </div>

        </div>

    </div>

