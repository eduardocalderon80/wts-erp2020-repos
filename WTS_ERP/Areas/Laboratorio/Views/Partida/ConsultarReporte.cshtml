<style type="text/css">

    .btnrecibido {
    background-color: white; 
    color: #A569BD; 
    border: 1px solid #A569BD;
    }
    .btnrecibido:hover {
    background-color: #A569BD;
    color: white;
    }
   
    thead {
        background-color: #1ab394;
        color:white;
    }
    th {
        padding: 8px;
        border: 1px solid #e7e7e7;      
        text-align: center;
    }  
    td {
        background-color: white;
         color: #676A6C;
        padding: 5px;
        border-bottom: 1px solid #e7e7e7;
       
        padding-left: 20px;
    }
</style>

<div class="row wrapper white-bg page-heading">
    <div id="content_header" class="ibox float-e-margins">
        <div id="divtituloprincipal" class="col-sm-6">
            <h2 class="font-bold">Laboratory Reports</h2>
        </div>
        <div id="divBotonesPrincipal" class="col-sm-6 text-right" style="vertical-align: middle;margin-top:15px;">
        </div>
    </div>
</div>


<div class="row wrapper-content gray-bg animated">

    <div id="content_form" class="ibox">

        <div class="ibox-title">
            <div class="form-horizontal row ">
                <div class="col-sm-6">
                    <h3 id="_title" class="text-navy bold">List</h3>
                </div>
                <div id="divbotonesprimarios" class="col-sm-6 text-right">
                    <button id="btnexport" class="btn btn-primary">
                        <span class="fa fa-file-excel-o"></span>
                        Export
                    </button>
                    <button id="btnsearch" title="Search" class="btn btn-info" onclick="req_get()">
                        <span class="fa fa-search"></span>
                        Search
                    </button>
                </div>
            </div>
        </div>

        <div id="formbusqueda" class="ibox-content">

            <div class="form-group">
                <div class="form-horizontal row">
                    <div class="col-sm-3">
                        <label class="control-label">Factory</label>
                        <select id="cbofabrica" class="form-control" style="width:100%" name="idfabrica" data-id="idcliente"></select>
                    </div>

                    <div class="col-sm-3">
                        <label class="control-label">Client</label>
                        <select id="cbocliente" class="form-control" style="width:100%" name="idcliente" data-id="idcliente"></select>
                    </div>
                    <div class="col-sm-3">
                        <label class="control-label">Status</label>
                        <select id="cbostatusfinal" class="form-control" style="width:100%" name="idcliente" data-id="idcliente"></select>
                    </div>
                    <div class="col-sm-3">
                        <label class="control-label">Type Test</label>                        
                        <select id="cbotipoprueba" class="form-control" style="width:100%" name="idtipoprueba" data-id="idtipoprueba"></select>
                    </div>
                </div>
            </div>

            <div class="form-group">
                <div class="form-horizontal row">                  
                    <div class="col-sm-3">
                        <label class="control-label">Start Date:</label>
                        <div id="divfechadesde">
                            <div class='input-group date'>
                                <input id="txtfechadesde" type='text' class='form-control' data-type="date" data-min="10" data-max="10">
                                <div id="divcalendariodesde" class="input-group-addon">
                                    <span class="fa fa-calendar"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <label class="control-label">End Date:</label>
                        <div id="divfechahasta">
                            <div class='input-group date'>
                                <input id="txtfechahasta" type='text' class='form-control' data-type="date" data-min="10" data-max="10">
                                <div id="divcalendariohasta" class="input-group-addon">
                                    <span class="fa fa-calendar"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <label class="control-label"># Technical Report</label>
                        <input type="text" id="txtreportetecnico" name="status" value="" placeholder="Reporte Tecnico" class="form-control" onkeyup="event_search(event)">
                    </div>
                    <div class="col-sm-3">
                        <label class="control-label"># Partida</label>
                        <input type="text" id="txtnumpartida" name="status" value="" placeholder="Numero Partida" class="form-control" onkeyup="event_search(event)">
                    </div>
                </div>
            </div>
            
        </div>
       
    </div>

    <div class="ibox">
        <div class="ibox-content">
            <div class="form-group">
                <div class="form-horizontal row">
                    <div id="tablecontentpartidas" class="col-sm-12">

                    </div>
                    <div class="col-sm-12 text-center ">
                        <label id="datos" class="hide">No hay datos</label>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>


