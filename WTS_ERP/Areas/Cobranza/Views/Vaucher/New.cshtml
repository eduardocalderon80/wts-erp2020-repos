<style type="text/css">

    #panelEncabezado_Vaucher .ibox {
        clear: both;
        margin-bottom: 5px !important;
        margin-top: 0;
        padding: 0;
    }

    #panelEncabezado_Vaucher .ibox-content {
        color: inherit;
        padding: 5px 20px 5px 10px !important;
        border-color: #e7eaec;
        border-image: none;
        border-style: solid solid none;
        border-width: 1px 0;
    }

    .grid-container {
        display: grid;
        grid-template-columns: 30% 10% 60%;
        grid-gap: 10px;
     
    }
</style>

<div class="row wrapper-content gray-bg animated" id="panelEncabezado_Vaucher">

